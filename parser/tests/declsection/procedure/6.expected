BEGIN SYMTABLE
 VAR
  NAME : a
  TYPE : integer
 TYPE : boolean
 BEGIN PROCEDURE func
  BEGIN SYMTABLE
   VAR
    NAME : arg2
    TYPE : integer
   VAR
    NAME : func
    TYPE : float
   VAR
    NAME : arg1
    TYPE : integer
  END SYMTABLE
  BEGIN STMT BLOCK
   BEGIN STMT ASSIGN
     arg1
    :=
      arg1
     +
      2
   END STMT ASSIGN
   BEGIN STMT ASSIGN
     arg2
    :=
      arg1
     +
      arg2
   END STMT ASSIGN
   BEGIN STMT ASSIGN
     func
    :=
     1
   END STMT ASSIGN
  END STMT BLOCK
 END PROCEDURE func
 TYPE : char
 TYPE : integer
 TYPE : float
END SYMTABLE
BEGIN STMT BLOCK
 BEGIN STMT ASSIGN
   a
  :=
   BEGIN FUNCTION func CALL
    Arg number 0
     1
    Arg number 1
     2
   END FUNCTION CALL
 END STMT ASSIGN
END STMT BLOCK
