BEGIN SYMTABLE
 VAR
  NAME : a
  TYPE : boolean
 VAR
  NAME : b
  TYPE : boolean
 TYPE : boolean
 TYPE : char
 TYPE : integer
 TYPE : float
END SYMTABLE
BEGIN STMT BLOCK
 BEGIN STMT ASSIGN
   a
  :=
   true
 END STMT ASSIGN
 BEGIN STMT ASSIGN
   b
  :=
   false
 END STMT ASSIGN
 BEGIN STMT ASSIGN
   a
  :=
    a
   and
    b
 END STMT ASSIGN
 BEGIN STMT ASSIGN
   a
  :=
    a
   or
    b
 END STMT ASSIGN
 BEGIN STMT ASSIGN
   a
  :=
    a
   xor
    b
 END STMT ASSIGN
END STMT BLOCK
