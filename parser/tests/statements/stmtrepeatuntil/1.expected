BEGIN SYMTABLE
 VAR
  NAME : a
  TYPE : integer
 VAR
  NAME : b
  TYPE : integer
 TYPE : boolean
 TYPE : char
 VAR
  NAME : i
  TYPE : integer
 TYPE : integer
 TYPE : float
END SYMTABLE
BEGIN STMT BLOCK
 BEGIN FOR LOOP
  FOR i :=
   1
  TO
   5
  DO
   BEGIN STMT BLOCK
    BEGIN REPEAT UNTIL LOOP
    REPEAT
     BEGIN STMT BLOCK
      BEGIN STMT ASSIGN
        a
       :=
         a
        +
         1
      END STMT ASSIGN
     END STMT BLOCK
    UNTIL
      a
     >
      i
   END STMT BLOCK
 END FOR LOOP
END STMT BLOCK
