BEGIN SYMTABLE
 TYPE : boolean
 TYPE : char
 VAR
  NAME : cnt
  TYPE : integer
 VAR
  NAME : i
  TYPE : integer
 VAR
  NAME : j
  TYPE : integer
 TYPE : integer
 TYPE : float
END SYMTABLE
BEGIN STMT BLOCK
 BEGIN FOR LOOP
  FOR i :=
   1
  TO
   5
  DO
   BEGIN STMT BLOCK
    BEGIN FOR LOOP
     FOR j :=
      i
     TO
      7
     DO
      BEGIN STMT BLOCK
       BREAK
      END STMT BLOCK
    END FOR LOOP
    BEGIN STMT ASSIGN
      cnt
     :=
       cnt
      +
       1
    END STMT ASSIGN
    BREAK
   END STMT BLOCK
 END FOR LOOP
END STMT BLOCK
