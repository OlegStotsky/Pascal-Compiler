BEGIN SYMTABLE
 VAR
  NAME : a
  TYPE : float
 VAR
  NAME : b
  TYPE : float
 TYPE : boolean
 TYPE : char
 TYPE : integer
 TYPE : float
 BEGIN PROCEDURE plus
  BEGIN SYMTABLE
   VAR
    NAME : first
    TYPE : integer
   VAR
    NAME : second
    TYPE : integer
  END SYMTABLE
  BEGIN STMT BLOCK
   BEGIN STMT ASSIGN
     a
    :=
      first
     +
      second
   END STMT ASSIGN
  END STMT BLOCK
 END PROCEDURE plus
END SYMTABLE
BEGIN STMT BLOCK
 BEGIN PROCEDURE CALL
  Arg number 0
   a
  Arg number 1
   b
  BEGIN PROCEDURE plus
   BEGIN SYMTABLE
    VAR
     NAME : first
     TYPE : integer
    VAR
     NAME : second
     TYPE : integer
   END SYMTABLE
   BEGIN STMT BLOCK
    BEGIN STMT ASSIGN
      a
     :=
       first
