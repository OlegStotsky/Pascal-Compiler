BEGIN SYMTABLE
 VAR
  NAME : a
  TYPE : integer
 VAR
  NAME : q
  TYPE : boolean
 VAR
  NAME : b
  TYPE : float
 TYPE : boolean
 VAR
  NAME : c
  TYPE : boolean
 TYPE : char
 TYPE : integer
 TYPE : float
END SYMTABLE
BEGIN STMT BLOCK
 BEGIN STMT ASSIGN
   c
  :=
     a
    <
     b
   and
     a
    >
     b
 END STMT ASSIGN
 BEGIN STMT ASSIGN
   q
  :=
     a
    >
     b
   or
     b
    <
     a
 END STMT ASSIGN
END STMT BLOCK
