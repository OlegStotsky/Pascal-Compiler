BEGIN SYMTABLE
 VAR
  NAME : a
  TYPE : boolean
 TYPE : boolean
 TYPE : char
 VAR
  NAME : i
  TYPE : integer
 TYPE : integer
 TYPE : float
END SYMTABLE
BEGIN STMT BLOCK
 BEGIN WHILE LOOP
  WHILE
   a
  DO
   BEGIN STMT ASSIGN
     i
    :=
      i
     +
      1
   END STMT ASSIGN
 END WHILE LOOP
END STMT BLOCK
