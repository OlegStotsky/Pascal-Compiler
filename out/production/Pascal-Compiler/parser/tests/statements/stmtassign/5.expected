BEGIN SYMTABLE
 TYPE a : RECORD
  BEGIN SYMTABLE
   VAR
    NAME : b
    BEGIN TYPE : RECORD
     BEGIN SYMTABLE
      VAR
       NAME : x
       TYPE : integer
     END SYMTABLE
    END TYPE : RECORD
  END SYMTABLE
 END TYPE : RECORD
 TYPE : boolean
 BEGIN PROCEDURE func
  BEGIN SYMTABLE
   VAR
    NAME : func
    TYPE a : RECORD
     BEGIN SYMTABLE
      VAR
       NAME : b
       BEGIN TYPE : RECORD
        BEGIN SYMTABLE
         VAR
          NAME : x
          TYPE : integer
        END SYMTABLE
       END TYPE : RECORD
     END SYMTABLE
    END TYPE : RECORD
  END SYMTABLE
  BEGIN STMT BLOCK
  END STMT BLOCK
 END PROCEDURE func
 TYPE : char
 TYPE : integer
 TYPE : float
END SYMTABLE
BEGIN STMT BLOCK
 BEGIN STMT ASSIGN
     BEGIN FUNCTION func CALL
     END FUNCTION CALL
    .
     b
   .
    x
  :=
   1
 END STMT ASSIGN
END STMT BLOCK
